{
    "clientId": "CMS",
    "clientSecret": "cms",
    "grant_type": "password",
    "scope": "auth LMS",
    "instanceUrl": "https://www.cyberu.com",
    "apiUrl": {
        "auth": {
            "baseUrl": "http://auth.dev.cyberu.io",
            "endpointUrl": {
                "domain": {
                    "fetch": "/domain",
                    "create": "/domain",
                    "delete": "/domain"
                },
                "user": {
                    "fetch": "/user/search",
                    "export": "/user/csv"
                }
            }
        },
        "lms": {
            "baseUrl": "https://lms-api.dev.cyberu.io/api/v1",
            "endpointUrl": {
                "parentCategory": {
                    "fetch": "/categories",
                    "fetchCategoryList":"/todofetchCategoryListUrl",
                    "fetchSubcategory":"/categories/subcategories_with_featured_courses",
                    "create":"/categories",
                    "update":"/categories/"
                },
                "coursePerCategory": {
                    "fetch": "/categories/with_courses_report"
                },
                "feature":{
                   "fetch": "/categories",
                   "add": "/courses"
                }
            }
        },
        "gateway": {
            "baseUrl": "https://gateway.dev.cyberu.io/api/v1/cms",
            "endpointUrl": {
                "transactions": {
                    "fetch": "/transaction_report",
                    "export": "/transaction_report/csv"
                },
                "courseRegistration": {
                    "fetch": "/courses",
                    "courseDetail": "/registrations"
                },
                "courseOnReview": {
                    "fetch": "/courses/in_review"
                },
                "course": {
                    "fetch": "/courses",
                    "export":"/courses/csv"
                }
            }
        }
    }
}